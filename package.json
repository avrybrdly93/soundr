  "bugs": {
    "url": "https://github.com/avrybrdly93/soundr/issues"
  },
  "homepage": "https://github.com/avrybrdly93/soundr#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "babel-preset-react-app": "^7.0.2",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "mongoose": "^5.4.17",
